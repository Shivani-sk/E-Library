<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>eLibrary</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>AdminLogin</servlet-name>
    <servlet-class>com.Jspiders.elibrary.first.AdminLogin</servlet-class>
  </servlet>
  <servlet-mapping>
  	
    <servlet-name>AdminLogin</servlet-name>
    <url-pattern>/AdminLogin</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>AddLibrarian</servlet-name>
    <servlet-class>com.Jspiders.elibrary.first.AddLibrarian</servlet-class>
  </servlet>
  <servlet-mapping>
  	
    <servlet-name>AddLibrarian</servlet-name>
    <url-pattern>/AddLibrarian</url-pattern>
  </servlet-mapping>
  
   <servlet>
    <servlet-name>Librarian</servlet-name>
    <servlet-class>com.Jspiders.elibrary.entity.Librarian</servlet-class>
  </servlet>
  <servlet-mapping>
  	
    <servlet-name>Librarian</servlet-name>
    <url-pattern>/Librarian</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>LibrarianCRUD</servlet-name>
    <servlet-class>com.Jspiders.elibrary.crud.LibrarianCRUD</servlet-class>
  </servlet>
  <servlet-mapping>
  	
    <servlet-name>LibrarianCRUD</servlet-name>
    <url-pattern>/LibrarianCRUD</url-pattern>
  </servlet-mapping>
  
   <servlet>
    <servlet-name>LibrarianLogin</servlet-name>
    <servlet-class>com.Jspiders.elibrary.first.LibrarianLogin</servlet-class>
  </servlet>
  <servlet-mapping>
  	
    <servlet-name>LibrarianLogin</servlet-name>
    <url-pattern>/LibrarianLogin</url-pattern>
  </servlet-mapping>
  
     <servlet>
    <servlet-name>DbOperations</servlet-name>
    <servlet-class>com.Jspiders.elibrary.db.DbOperations</servlet-class>
  </servlet>
  <servlet-mapping>
  	
    <servlet-name>DbOperations</servlet-name>
    <url-pattern>/DbOperations</url-pattern>
  </servlet-mapping>
  
  
</web-app>